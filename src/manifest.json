{
  "name": "Bookmark Index",
  "version": "0.0.1",
  "description": "游览器书签加强插件",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup/popup.html",
    "default_title": "Click me"
  },
  "background": {
    "service_worker": "scripts/background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at": "document_start",
      "js": ["./scripts/content/inject.js"]
    }
  ],
  "host_permissions": ["<all_urls>"]
}
